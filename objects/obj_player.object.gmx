        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handle input

//Camera
dir += -90 * global.sensitivity*(mouse_delta_x)/window_get_width();
pitch = clamp(pitch + (90 * global.sensitivity*(mouse_delta_y)/window_get_height()), -89.8, 89.8);

// Movement
var w = keyboard_check(ord("W"));
var a = keyboard_check(ord("A"));
var s = keyboard_check(ord("S"));
var d = keyboard_check(ord("D"));

if(w &amp;&amp; !s) {
    if(a &amp;&amp; !d) {
        direction = dir+45;
    } else if(d &amp;&amp; !a) {
        direction = dir-45;
    } else {
        direction = dir;
    }
} else if(s &amp;&amp; !w) {
    if(a &amp;&amp; !d) {
        direction = dir+135;
    } else if(d &amp;&amp; !a) {
        direction = dir-135;
    } else {
        direction = dir+180;
    }
} else if(a &amp;&amp; !d) {
    direction = dir+90;
} else if (d &amp;&amp; !a) {
    direction = dir-90;
}

if(w || a || s || d) {
    speed = .1;
} else {
    speed = 0;
}

if(keyboard_check(vk_space)) {
    z += .1;
}
if(keyboard_check(vk_shift)) {
    z -= .1;
}
</string>
          </argument>
        </arguments>
      </action>
